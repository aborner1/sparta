###################################
# Simulation parameters
###################################
seed	    	    847384
dimension   	    3
global		    nrho ${nden} fnum ${Fnum} gridcut -1 surfmax 2000 surfgrid persurf comm/style all
global		    nrho 5.561008e+17 fnum ${Fnum} gridcut -1 surfmax 2000 surfgrid persurf comm/style all
global		    nrho 5.561008e+17 fnum 8403300977777.78 gridcut -1 surfmax 2000 surfgrid persurf comm/style all
timestep            ${tstep}
timestep            3.0388157796952e-06

###################################
# Grid generation
###################################
boundary	    o o o
create_box          ${xmin} ${xmax} ${ymin} ${ymax} ${zmin} ${zmax}
create_box          -2.505 ${xmax} ${ymin} ${ymax} ${zmin} ${zmax}
create_box          -2.505 0.895 ${ymin} ${ymax} ${zmin} ${zmax}
create_box          -2.505 0.895 -2.5 ${ymax} ${zmin} ${zmax}
create_box          -2.505 0.895 -2.5 2.5 ${zmin} ${zmax}
create_box          -2.505 0.895 -2.5 2.5 -2.5 ${zmax}
create_box          -2.505 0.895 -2.5 2.5 -2.5 2.5
Created orthogonal box = (-2.505 -2.5 -2.5) to (0.895 2.5 2.5)
create_grid 	    ${xncells} ${yncells} ${zncells} block * * *
create_grid 	    10 ${yncells} ${zncells} block * * *
create_grid 	    10 15 ${zncells} block * * *
create_grid 	    10 15 15 block * * *
