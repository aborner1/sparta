surf_collide	    1 diffuse ${surftemp} 1.0
surf_collide	    1 diffuse 615 1.0
#surf_react          1 prob catalytic.surf
surf_modify         1 collide 1 #react 1

compute             88 surf all all etot
fix                 88 ave/surf all 1 100 100 c_88[*] ave one

###################################
# Boundary conditions
###################################
