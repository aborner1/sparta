SPARTA (20 Nov 2020)
# advect particles on uniform Cartesian grid
# single-step moves that cross grid cell boundaries are detected
# particle effectively moves from cell to cell
# particles reflect off global box boundaries

###################################
# Trajectory inputs
###################################
variable            V equal 5854.71
variable            nden equal 5.561008e17
variable            temp equal 163.8

variable            surftemp equal 615.0
variable            alpha equal 17.146
variable            beta equal -0.511

###################################
# Gas parameters (C02)
###################################
variable            mue equal 1.380E-5
variable            mass equal 73.1E-27
variable            gamma equal 1.28
variable            rho equal ${nden}*${mass}
variable            rho equal 5.561008e+17*${mass}
variable            rho equal 5.561008e+17*7.31e-26

###################################
# Constants
###################################
variable            boltz equal 1.380658E-23
variable            pi equal 3.14159

###################################
# Simulation initialization standards
###################################
variable            ppc equal 2500.0
variable            cpmfp equal 5.0

###################################
# Parameter calculations
###################################
variable            Vx equal ${V}*cos(${alpha}*2*PI/360)*cos(${beta}*2*PI/360)
variable            Vx equal 5854.71*cos(${alpha}*2*PI/360)*cos(${beta}*2*PI/360)
variable            Vx equal 5854.71*cos(17.146*2*PI/360)*cos(${beta}*2*PI/360)
variable            Vx equal 5854.71*cos(17.146*2*PI/360)*cos(-0.511*2*PI/360)
variable            Vy equal ${V}*sin(${alpha}*2*PI/360)*sin(${beta}*2*PI/360)
variable            Vy equal 5854.71*sin(${alpha}*2*PI/360)*sin(${beta}*2*PI/360)
variable            Vy equal 5854.71*sin(17.146*2*PI/360)*sin(${beta}*2*PI/360)
variable            Vy equal 5854.71*sin(17.146*2*PI/360)*sin(-0.511*2*PI/360)
variable            Vz equal ${V}*sin(${alpha}*2*PI/360)*cos(${beta}*2*PI/360)
variable            Vz equal 5854.71*sin(${alpha}*2*PI/360)*cos(${beta}*2*PI/360)
variable            Vz equal 5854.71*sin(17.146*2*PI/360)*cos(${beta}*2*PI/360)
variable            Vz equal 5854.71*sin(17.146*2*PI/360)*cos(-0.511*2*PI/360)

variable            cbar equal sqrt(8./${pi}*${boltz}*${temp}/${mass})
variable            cbar equal sqrt(8./3.14159*${boltz}*${temp}/${mass})
variable            cbar equal sqrt(8./3.14159*1.380658e-23*${temp}/${mass})
variable            cbar equal sqrt(8./3.14159*1.380658e-23*163.8/${mass})
variable            cbar equal sqrt(8./3.14159*1.380658e-23*163.8/7.31e-26)
variable            mfp equal 1/(sqrt(2)*${pi}*(4.684e-10)^2*${nden}*(273/${temp})^(0.287))
variable            mfp equal 1/(sqrt(2)*3.14159*(4.684e-10)^2*${nden}*(273/${temp})^(0.287))
variable            mfp equal 1/(sqrt(2)*3.14159*(4.684e-10)^2*5.561008e+17*(273/${temp})^(0.287))
variable            mfp equal 1/(sqrt(2)*3.14159*(4.684e-10)^2*5.561008e+17*(273/163.8)^(0.287))
variable            uspeed equal sqrt(${gamma}*${boltz}*${temp}/${mass})
variable            uspeed equal sqrt(1.28*${boltz}*${temp}/${mass})
variable            uspeed equal sqrt(1.28*1.380658e-23*${temp}/${mass})
variable            uspeed equal sqrt(1.28*1.380658e-23*163.8/${mass})
variable            uspeed equal sqrt(1.28*1.380658e-23*163.8/7.31e-26)

variable            xmin equal -2.505
variable            xmax equal 0.895
variable            ymin equal -2.5
variable            ymax equal 2.5
variable            zmin equal -2.5
variable            zmax equal 2.5

variable            xncells equal floor((${xmax}-${xmin})/${mfp}*${cpmfp})
variable            xncells equal floor((0.895-${xmin})/${mfp}*${cpmfp})
variable            xncells equal floor((0.895--2.505)/${mfp}*${cpmfp})
variable            xncells equal floor((0.895--2.505)/1.59322683434267*${cpmfp})
variable            xncells equal floor((0.895--2.505)/1.59322683434267*5)
variable            yncells equal floor((${ymax}-${ymin})/${mfp}*${cpmfp})
variable            yncells equal floor((2.5-${ymin})/${mfp}*${cpmfp})
variable            yncells equal floor((2.5--2.5)/${mfp}*${cpmfp})
variable            yncells equal floor((2.5--2.5)/1.59322683434267*${cpmfp})
variable            yncells equal floor((2.5--2.5)/1.59322683434267*5)
variable            zncells equal floor((${zmax}-${zmin})/${mfp}*${cpmfp})
variable            zncells equal floor((2.5-${zmin})/${mfp}*${cpmfp})
variable            zncells equal floor((2.5--2.5)/${mfp}*${cpmfp})
variable            zncells equal floor((2.5--2.5)/1.59322683434267*${cpmfp})
variable            zncells equal floor((2.5--2.5)/1.59322683434267*5)

variable            Fnum equal  ${nden}*(${xmax}-${xmin})*(${ymax}-${ymin})*(${zmax}-${zmin})/${ppc}/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(${xmax}-${xmin})*(${ymax}-${ymin})*(${zmax}-${zmin})/${ppc}/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895-${xmin})*(${ymax}-${ymin})*(${zmax}-${zmin})/${ppc}/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(${ymax}-${ymin})*(${zmax}-${zmin})/${ppc}/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(2.5-${ymin})*(${zmax}-${zmin})/${ppc}/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(2.5--2.5)*(${zmax}-${zmin})/${ppc}/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(2.5--2.5)*(2.5-${zmin})/${ppc}/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(2.5--2.5)*(2.5--2.5)/${ppc}/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(2.5--2.5)*(2.5--2.5)/2500/${xncells}/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(2.5--2.5)*(2.5--2.5)/2500/10/${yncells}/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(2.5--2.5)*(2.5--2.5)/2500/10/15/${zncells}
variable            Fnum equal  5.561008e+17*(0.895--2.505)*(2.5--2.5)*(2.5--2.5)/2500/10/15/15

variable            tstep equal (-${xmin}+${xmax})/${Vx}/${xncells}/20
variable            tstep equal (--2.505+${xmax})/${Vx}/${xncells}/20
variable            tstep equal (--2.505+0.895)/${Vx}/${xncells}/20
variable            tstep equal (--2.505+0.895)/5594.28449516119/${xncells}/20
variable            tstep equal (--2.505+0.895)/5594.28449516119/10/20

###################################
# Print variable values to log file
###################################
print               " Velocity  = ${V}"
 Velocity  = 5854.71
print               " Density  = ${rho}"
